#include<stdio.h>
#include<stdlib.h>

int main(){
    int i,n,s, flag;
    flag=0;
    printf("Enter size of array");
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);

    }
    printf("element for the search");
    scanf("%d",&s);

    for(i=0;i<n;i++){
        if(a[i]==s){
            printf("element in index:%d",i);
            flag=1;
        }
    }
    if(flag==0){
        printf("not found");
    }
}

output:-
Enter size of array4
1
2
3
4
element for the search3
element in index:2
Process returned 0 (0x0)   execution time : 6.466 s
